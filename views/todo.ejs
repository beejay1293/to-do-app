<body>

  <h1>My Todo List</h1>
  <div id="todo-table">
<form >
<input type="text" name="item" placeholder="Add new item..."  required  />
<button type="submit" >Add Item</button>
</form>
<ul>
  <% for (var i = 0 ; i < todo.length; i++) { %>
     <li><%= todo[i].todos %></li>

  <%}%>
</ul>
  </div>

</body>




